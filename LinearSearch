public class Main {
    public static void main(String[] args) {
        int [] arr={1,8,5,7,3,9,3,2};

        int index=LinearSearch(arr,2);
        System.out.println(index);
    }

    private static int LinearSearch(int[] array,int target){
        for (int i=0;i<array.length;i++){
            if(array[i]==target){
                return i;
            }
        }
        return -1;
    }
}
